<h1>Manage Staffs</h1>

<p><%= link_to "Add Staff", signup_path %></p>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">

  <tr>
    <th>Full Name</th>
    <th>Username</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.first_name %> <%= user.last_name %></td>
    <td><%= user.username %></td>
  </tr>
<% end %>
</table>


